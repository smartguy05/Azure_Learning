{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "module": "esnext",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "@app/*": [ "./src/app/*" ],
      "@models/*": [ "./src/models/*" ],
      "@services/*": [ "./src/app/services/*" ],
      "@environments/*": [ "./src/environments/*" ],
      "@components/*": [ "./src/app/components/*" ]
    }
  }
}
